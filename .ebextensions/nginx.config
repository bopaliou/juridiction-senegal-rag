files:
  "/etc/nginx/conf.d/custom.conf":
    mode: "000644"
    owner: root
    group: root
    content: |
      client_max_body_size 20M;
      proxy_request_buffering off;
      proxy_buffering off;

container_commands:
  01_migrate:
    command: "source /var/app/venv/*/bin/activate && python src/server.py"
    leader_only: true
